{
  "get": {
    "parameters": [
      {
        "name": "id",
        "in": "path",
        "type": "string",
        "required": true
      }
    ],
    "responses": {
      "200": {
        "description": "Get <%= objName %> with required id",
        "schema": {
          "$ref": "#/definitions/<%= objName %>"
        }
      },
      "default": {
        "description": "unexpected error",
        "schema": {
          "$ref": "#/definitions/Error"
        }
      }
    }
  }
}